{
  "name": "@ashleycaselli/gulp-stylelint",
  "version": "0.0.0-semantic-release",
  "description": "Gulp plugin for running Stylelint results through various reporters.",
  "main": "src/index.js",
  "files": [
    "/src/*.js"
  ],
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.js\"",
    "tape": "tape \"test/*.spec.js\"",
    "test": "npm run lint && npm run tape",
    "prepublishOnly": "npm test"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashleycaselli/gulp-stylelint.git"
  },
  "keywords": [
    "gulpplugin",
    "stylelint",
    "postcss",
    "css"
  ],
  "author": "Oleg Sklyanchuk <sklyanchuk@gmail.com> (http://olegskl.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashleycaselli/gulp-stylelint/issues"
  },
  "homepage": "https://github.com/ashleycaselli/gulp-stylelint",
  "engines": {
    "node": ">=14.18.1"
  },
  "peerDependencies": {
    "stylelint": "^14.0.1"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "fancy-log": "^2.0.0",
    "plugin-error": "^2.0.0",
    "source-map": "^0.7.3",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "semantic-release-preconfigured-conventional-commits": "1.1.6",
    "eslint": "7.32.0",
    "eslint-config-stylelint": "11.1.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.3.0",
    "gulp-concat": "2.6.1",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "sinon": "14.0.0",
    "stylelint": "14.0.1",
    "tape": "5.5.3"
  }
}
